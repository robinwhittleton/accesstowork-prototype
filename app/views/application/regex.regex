s/\{\{<(.*)\}\}/{% extends "\1.html" %}/g

\{\{\$([^`}]*)\}\}
{% block \1 %}

\{\{/([^\}]*)\}\}
{% endblock %}

\{\{>(.*)\}\}
{% include "\1.html" %}


sed -i '' 's/{{<\(.*\)}}/{% extends "\1.html" %}/g' index.html 

sed -i '' 's/{{$\(.*\)}}/{% block \1 %}/g' index.html

sed -i '' 's/{{\/\(.*\)}}/{% endblock %}/g' index.html

sed -i '' 's/{{>\(.*\)}}/{% include "\1.html" %}/g' index.html
